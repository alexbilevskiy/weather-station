sudo ln -s /opt/src/station/exporter/metrics.py /var/www/html/metrics
sudo chmod 777 /var/www/html/metrics
sudo ln -s /opt/src/station/exporter/metrics_raw.py /var/www/html/metrics_raw.py
sudo chmod 777 /var/www/html/metrics_raw.py

sudo ln -s /opt/src/station/systemd/led-clock.service /etc/systemd/system/led-clock.service
sudo ln -s /opt/src/station/systemd/sensors-exporter.service /etc/systemd/system/sensors-exporter.service

